   "source": [
    "from multiprocessing import Process\n",
    "\n",
    "def func():\n",
    "  for i in range(10000):\n",
    "    print(f'func{i}')\n",
    "\n",
    "p = Process(target=func)\n",
    "p.start()\n",
    "\n",
    "\n",
    "class MyProcess(Process):\n",
    "  def run(self):\n",
    "    for i in range(10000):\n",
    "      print(f'run{i}')\n",
    "p2 = MyProcess()\n",
    "p2.start()\n"
   ]
  }
